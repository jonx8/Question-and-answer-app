{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run openapi-generate-questions && npm run openapi-generate-users && vite",
    "build": "vue-tsc --noEmit && vite build",
    "openapi-generate-questions": "npx @openapitools/openapi-generator-cli generate -i http://localhost:8082/questions/v3/api-docs -g typescript-axios -o src/api/generated/questions",
    "openapi-generate-users" : "npx @openapitools/openapi-generator-cli generate -i http://localhost:8082/users/v3/api-docs -g typescript-axios -o src/api/generated/users",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@dsb-norge/vue-keycloak-js": "^2.4.0",
    "@mdi/font": "7.4.47",
    "axios": "^1.7.4",
    "core-js": "^3.37.1",
    "keycloak-js": "^25.0.4",
    "roboto-fontface": "*",
    "vue": "^3.4.31",
    "vuetify": "^3.6.11"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@openapitools/openapi-generator-cli": "^2.13.5",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "sass": "1.77.6",
    "typescript": "^5.4.2",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.3",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.26"
  }
}
